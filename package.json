{
  "$schema": "https://json.schemastore.org/package",
  "name": "@mark1russell7/ecosystem",
  "version": "1.0.0",
  "description": "Ecosystem manifest - source of truth for all @mark1russell7 packages",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./manifest": {
      "types": "./dist/manifest.d.ts",
      "import": "./dist/manifest.js"
    }
  },
  "files": [
    "dist",
    "src",
    "ecosystem.manifest.json"
  ],
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .tsbuildinfo"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^22.0.0",
    "@mark1russell7/cue": "github:mark1russell7/cue#main"
  },
  "keywords": [
    "ecosystem",
    "manifest",
    "packages",
    "registry"
  ],
  "license": "MIT",
  "sideEffects": false
}
